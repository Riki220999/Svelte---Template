{
  "name": "svlte",
  "version": "1.0.0",
  "description": "The default [Sapper](https://github.com/sveltejs/sapper) template, with branches for Rollup and webpack. To clone it and get started:",
  "main": "rollup.config.js",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build",
    "export": "sapper export",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "newinstall": "rm -rf node_module && rm package-lock.json && npm install"
  },
  "author": "riki setiyo pambudi",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "cypress": "^3.3.0",
    "polka": "^0.5.2",
    "sirv": "^0.4.2"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "svelte-loader": "^2.9.0",
    "webpack": "^4.7.0"
  }
}
